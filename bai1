#include <stdio.h>
#include <math.h>

int main(void) {
    int n; // Khởi tạo n với giá trị mặc định

    printf("Nhap vao mot so nguyen duong n: ");
    scanf("%d", &n);

    printf("Cac so chinh phuong nho hon %d la:\n", n);

    for (int i = 1; i < n; i++) {
        int squareRoot = sqrt(i);
        if (squareRoot * squareRoot == i) {
            printf("%d ", i);
        }
    }

    printf("\n");

    return 0;
}
